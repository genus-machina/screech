{
  "name": "@genus-machina/screech",
  "version": "1.0.2",
  "description": "Raspberry Pi utilities",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc",
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "pretest": "npm run lint",
    "prepublishOnly": "npm run compile",
    "test": "nyc ava -v"
  },
  "author": "Simplex Rimor <simplex.rimor@gamil.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.117",
    "@types/onoff": "^3.2.1",
    "@types/sinon": "^7.0.2",
    "@types/uuid": "^3.4.4",
    "ava": "^1.0.0-rc.1",
    "lodash": "^4.17.11",
    "nyc": "^13.1.0",
    "sinon": "^7.1.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "onoff": "^3.2.2",
    "suncalc": "^1.8.0",
    "uuid": "^3.3.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "failWithoutAssertions": false,
    "files": [
      "test/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "fuctions": 100,
    "lines": 100,
    "statements": 100,
    "exclude": [
      "dist/**/*.js",
      "test/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "source-map-support/register"
    ]
  }
}
